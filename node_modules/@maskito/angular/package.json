{
  "name": "@maskito/angular",
  "version": "3.0.1",
  "description": "The Angular-specific Maskito's library",
  "keywords": [
    "mask",
    "inputmask",
    "input-mask",
    "text-mask",
    "input-formatting",
    "angular"
  ],
  "homepage": "https://maskito.dev",
  "bugs": "https://github.com/taiga-family/maskito/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/taiga-family/maskito.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Nikita Barsukov",
    "email": "nikita.s.barsukov@gmail.com",
    "url": "https://github.com/nsbarsukov"
  },
  "contributors": [
    "Alex Inkin <alexander@inkin.ru> (https://twitter.com/waterplea)",
    "Maksim Ivanov <splincodewd@yandex.ru>",
    "Vladimir Potekhin <vladimir.potekh@gmail.com>",
    "Nikita Barsukov <nikita.s.barsukov@gmail.com>"
  ],
  "dependencies": {
    "tslib": "2.6.3"
  },
  "peerDependencies": {
    "@angular/core": ">=16.0.0",
    "@angular/forms": ">=16.0.0",
    "@maskito/core": "^3.0.1"
  },
  "ng-update": {
    "packageGroup": [
      "@maskito/core",
      "@maskito/kit"
    ]
  },
  "module": "fesm2022/maskito-angular.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/maskito-angular.mjs",
      "esm": "./esm2022/maskito-angular.mjs",
      "default": "./fesm2022/maskito-angular.mjs"
    }
  },
  "sideEffects": false
}